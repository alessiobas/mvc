                    <li><a href="{{ path("proj-home") }}">Project</a></li>
                    <li><a href="{{ path("debug") }}">Debug</a></li>
            </nav>
        </div>

        {% block content %}
        Def content
        {% endblock %}

        <div class="sitefooter">
            <p>Created by Alessio Basile &copy; 2022</p>
        </div>
    </body>
</html>
